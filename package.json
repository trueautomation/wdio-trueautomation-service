{
  "name": "wdio-trueautomation-service",
  "description": "TrueAutomation.IO integration for webdriver.io",
  "main": "index.js",
  "version": "0.3.0",
  "keywords": [
    "trueautomation",
    "trueautomation.io",
    "automation",
    "testing",
    "qa",
    "wdio-service",
    "wdio-plugin",
    "wdio"
  ],
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rm -rf ./build",
    "compile": "babel lib/ -d build/",
    "eslint": "eslint ./lib",
    "release": "npm run build && np patch",
    "release:patch": "npm run build && np patch",
    "release:minor": "npm run build && np minor",
    "release:major": "npm run build && np major",
    "test": "run-s eslint",
    "prepublish": "npm prune",
    "watch": "npm run compile -- --watch"
  },
  "author": {
    "name": "Softesis Inc.",
    "email": "info@trueautomation.io",
    "url": "http://trueautomation.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trueautomation/wdio-trueautomation-service.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "np": "^3.0.4",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "is-port-reachable": "^2.0.0",
    "node-fetch": "^2.2.0",
    "which": "^1.3.1"
  }
}
